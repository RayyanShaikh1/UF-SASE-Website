{
  "name": "@template/basic",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "db:push && vinxi build",
    "start": "vinxi start",
    "fix": "concurrently \"bun run lint:js:fix\" \"bun run lint:fmt:fix\"",
    "generate": "bun run src/client/assets/generateImageMaps.ts",
    "lint": "concurrently \"bun run lint:ts\" \"bun run lint:js\" \"bun run lint:fmt\"",
    "lint:ts": "tsc -b tsconfig.json",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:fmt": "prettier --check .",
    "lint:fmt:fix": "prettier --write .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "clean": "git clean -xdf -e .env -e local.db",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:local:init": "node scripts/initDb.mjs",
    "db:local:seed": "node scripts/seed.mjs",
    "db:studio": "drizzle-kit studio --port 3003 --verbose",
    "db:all": "bun db:push && bun db:local:init && bun db:local:seed && bun db:generate && bun db:studio"
  },
